{
  "version": 2,
  "name": "outreach-process-manager",
  "builds": [
    {
      "src": "apps/outreach-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist",
        "framework": "vite"
      }
    },
    {
      "src": "apps/outreach-process-manager/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/audit-log",
      "dest": "apps/outreach-process-manager/api/audit-log.js"
    },
    {
      "src": "/api/audit-log-test",
      "dest": "apps/outreach-process-manager/api/audit-log-test.js"
    },
    {
      "src": "/api/promote",
      "dest": "apps/outreach-process-manager/api/promote.js"
    },
    {
      "src": "/api/validate",
      "dest": "apps/outreach-process-manager/api/validate.js"
    },
    {
      "src": "/api/validate-test",
      "dest": "apps/outreach-process-manager/api/validate-test.js"
    },
    {
      "src": "/api/ingest",
      "dest": "apps/outreach-process-manager/api/ingest.js"
    },
    {
      "src": "/api/logs/(.*)",
      "dest": "apps/outreach-process-manager/api/logs/[logId].js"
    },
    {
      "src": "/",
      "dest": "apps/outreach-ui/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "apps/outreach-ui/dist/$1"
    }
  ],
  "functions": {
    "apps/outreach-process-manager/api/audit-log.js": {
      "maxDuration": 30
    },
    "apps/outreach-process-manager/api/promote.js": {
      "maxDuration": 30
    },
    "apps/outreach-process-manager/api/validate.js": {
      "maxDuration": 30
    },
    "apps/outreach-process-manager/api/ingest.js": {
      "maxDuration": 30
    },
    "apps/outreach-process-manager/api/logs/[logId].js": {
      "maxDuration": 15
    }
  },
  "env": {
    "MCP_API_URL": "@mcp_api_url",
    "NEON_DB_URL": "@neon_db_url",
    "COMPOSIO_API_KEY": "@composio_api_key",
    "COMPOSIO_BASE_URL": "@composio_base_url",
    "DOCTRINE_HASH": "@doctrine_hash",
    "NODE_ENV": "production",
    "VITE_API_URL": "https://outreach-process-manager.vercel.app"
  },
  "buildCommand": "cd apps/outreach-ui && npm install && npm run build",
  "installCommand": "cd apps/outreach-ui && npm install",
  "outputDirectory": "apps/outreach-ui/dist",
  "framework": "vite"
}