      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "neon-marketing-db"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH renewal_calc AS (\n  SELECT \n    company_unique_id,\n    company_name,\n    website_url,\n    CASE \n      WHEN renewal_month IS NOT NULL THEN\n        CASE \n          WHEN EXTRACT(MONTH FROM CURRENT_DATE) <= renewal_month THEN\n            DATE_TRUNC('year', CURRENT_DATE) + (renewal_month - 1 || ' months')::INTERVAL\n          ELSE\n            DATE_TRUNC('year', CURRENT_DATE + INTERVAL '1 year') + (renewal_month - 1 || ' months')::INTERVAL\n        END\n      ELSE NULL\n    END AS next_renewal_date\n  FROM marketing.company_master\n  WHERE renewal_month IS NOT NULL\n),\nslot_status AS (\n  SELECT \n    company_unique_id,\n    COUNT(*) FILTER (WHERE is_filled = true) AS slots_filled\n  FROM marketing.company_slot\n  GROUP BY company_unique_id\n)\nSELECT \n  rc.company_name,\n  rc.next_renewal_date,\n  EXTRACT(DAY FROM (rc.next_renewal_date - CURRENT_DATE))::INTEGER AS days_until_renewal,\n  COALESCE(ss.slots_filled, 0) AS slots_filled,\n  rc.website_url\nFROM renewal_calc rc\nLEFT JOIN slot_status ss ON rc.company_unique_id = ss.company_unique_id\nWHERE rc.next_renewal_date IS NOT NULL\n  AND rc.next_renewal_date >= CURRENT_DATE\n  AND rc.next_renewal_date <= CURRENT_DATE + INTERVAL '120 days'\nORDER BY days_until_renewal ASC\nLIMIT 50;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "üìÖ Renewal Pipeline ‚Äî Next 120 Days",
