{
  "tool_id": "parser",
  "name": "Parser",
  "blueprint_id": "04.svg.marketing.outreach.v1",
  "barton_id": "04.04.02.04.10000.004",
  "altitude": 14000,
  "version": "1.0.0",
  "description": "PDF / Doc parser for extracting structured data",
  "neon_tables": {
    "output": "intake.company_raw_intake",
    "audit": "shq.audit_log",
    "error": "shq.error_master"
  },
  "dependencies": {
    "pypdf2": "required",
    "python_docx": "required",
    "neon_db": "required"
  },
  "kill_switch": {
    "enabled": true,
    "condition": "parse_failures > 20",
    "action": "halt_and_notify"
  },
  "environment_variables": ["NEON_CONNECTION_STRING"],
  "api_endpoints": {
    "parse": "/api/parser/parse",
    "health": "/api/parser/health"
  },
  "n8n_integration": {
    "webhook_url": "/hooks/parser",
    "event_types": ["parse.requested", "parse.completed"],
    "response_format": "heir_orbt"
  }
}
