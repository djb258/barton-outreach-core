  "theme": "obsidian",
  "baseFontSize": 16,
  "cssTheme": "Default",
  "enabledCssSnippets": [],
  "translucency": false,
  "foldHeading": true,
  "foldIndent": true,
  "showLineNumber": false,
  "promptDelete": true,
  "spellcheck": true,
