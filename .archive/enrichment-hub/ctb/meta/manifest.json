{
  "manifest": {
    "name": "enricha-vision",
    "version": "1.0.0",
    "description": "AI-powered enrichment and vision tracking system",
    "ctb_version": "1.0.0",
    "created": "2025-11-07",
    "updated": "2025-11-07",
    "repository": {
      "type": "git",
      "url": "https://github.com/djb258/enricha-vision.git"
    },
    "system": {
      "heir_format": "HEIR-YYYY-MM-SYSTEM-MODE-VN",
      "process_id_format": "PRC-SYSTEM-EPOCHTIMESTAMP",
      "system_id": "ENRICHA"
    },
    "technology_stack": {
      "frontend": {
        "framework": "React",
        "language": "TypeScript",
        "build_tool": "Vite",
        "styling": "Tailwind CSS",
        "ui_library": "shadcn/ui"
      },
      "backend": {
        "services": ["Firebase", "Neon PostgreSQL"],
        "authentication": "Firebase Auth",
        "api_pattern": "RESTful"
      },
      "integrations": {
        "ai": ["Gemini Pro", "OpenAI"],
        "automation": "Composio MCP",
        "version_control": "GitHub"
      },
      "infrastructure": {
        "hosting": "TBD",
        "database": ["Firebase Firestore", "Neon PostgreSQL"],
        "storage": "Firebase Storage"
      }
    },
    "ctb_branches": [
      "sys",
      "ai",
      "data",
      "docs",
      "ui",
      "meta"
    ],
    "features": {
      "authentication": true,
      "ai_integration": true,
      "real_time_updates": true,
      "file_storage": true,
      "analytics": true
    },
    "compliance": {
      "ctb_enabled": true,
      "doctrine_enforcement": true,
      "auto_remediate": true,
      "audit_frequency": "monthly",
      "min_compliance_score": 70
    },
    "maintainers": [
      "CTB Standards Team"
    ],
    "license": "MIT"
  }
}
